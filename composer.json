{
  "name": "setbased/phing-extensions",
  "description": "A collection of Phing extensions",
  "keywords": [
    "Phing",
    "Extensions"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "phing/phing": "^3.0.0-RC4",
    "setbased/helper-program-execution": "^1.2.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Phing\\Task\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
