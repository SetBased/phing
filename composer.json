{
  "name": "setbased/phing-extensions",
  "description": "A collection of Phing extensions",
  "keywords": [
    "Phing",
    "Extensions"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "phing/phing": "3.0.0a3",
    "setbased/helper-program-execution": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": ["src"]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
